{
    "name": "Price scraper",
    "description": "Scrape price of products from the webpage",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "background": {"service_worker": "background.js"},
    "permissions": ["activeTab", "scripting", "tabs"],
    "host_permissions": ["*://*.kroger.com/*", "*://*.walmart.com/*", "*://*.target.com/*"]
}